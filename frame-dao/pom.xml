            <systemPath>${project.basedir}/../frame-web/src/main/webapp/WEB-INF/lib/fddl-sequence-0.2.jar</systemPath>
        </dependency>
        <!-- https://mvnrepository.com/artifact/com.baomidou/mybatis-plus -->
