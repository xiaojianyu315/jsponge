        </dependency>

        <dependency>
            <groupId>org.springframework</groupId>
            <artifactId>spring-test</artifactId>
            <version>${spring.version}</version>
        </dependency>

        <dependency>
            <groupId>org.easy</groupId>
            <artifactId>frame-common</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>com.jfbank.fddl</groupId>
            <artifactId>fddl-sequence</artifactId>
            <version>0.2</version>
